#include<iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter two numbers :"<<endl;
    cin>>a>>b;
    if(a>b)
        cout<<"greatest among two numbers is = "<<a;
    else if(a==b)
        cout<<"both the numbers are equal !"<<endl;
    else
        cout<<"greatest among two numbers is = "<<b;
}
